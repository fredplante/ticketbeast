<h1><%= @concert.title %></h1>
<h2><%= @concert.subtitle %></h2>
<p><%= @concert.date.strftime("%B %e, %Y") %></p>
<p>Doors at: <%= @concert.date.strftime("%I:%M%P") %></p>
<p><%= number_with_precision(@concert.ticket_price.to_f / 100, precision: 2) %></p>
<p><%= @concert.venue %></p>
<p><%= @concert.venue_address %></p>
<p><%= @concert.city %>, <%= @concert.state %> <%= @concert.zip %></p>
<p><%= @concert.additional_information %></p>